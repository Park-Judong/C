#include <stdio.h>
 
int main()
{
    int num1 = 10;
    int num2;
    int num3 = 15;
    int num4 = 20;
    bool num5 = true;
 
    num2 = num1==10 ? 100 : 200; 
 
    printf("%d\n", num1 != 5);     // 1: num1이 5와 다른지
 
    printf("%d\n", num1 > 10);     // 0: num1이 10보다 큰지
    printf("%d\n", num1 < 10);     // 0: num1이 10보다 작은지
 
    printf("%d\n", num1 >= 10);    // 1: num1이 10보다 크거나 같은지
    printf("%d\n", num1 <= 10);    // 1: num1이 10보다 작거나 같은지
 
    printf("%d\n", num2);

    printf("%d\n", 1 && 1);    // 1: 1 AND 1
    printf("%d\n", 1 || 0);    // 1 OR 0
    printf("%d\n", 0 && 1);    // 0: 0 AND 1
    printf("%d\n", 0 && 0);     // 0: 0 AND 0
 
    printf("%d\n", !(2 && 3));    // 2 AND 3

    printf("%d\n", num1 > num2 || num3 < num4);

    printf("%s\n", num1 && num2 ? "참" : "거짓");

    if (num5 == true)        
        printf("참\n");    
    else
        printf("거짓\n");

    return 0;
}
