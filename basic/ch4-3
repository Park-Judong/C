#include <stdio.h>
//brakets {} : A Range of codes
//tab: easier to read codes for users
int main(){
    int i;
    for(i=0; i<10; i++){
        printf("Hello %d times \n", i+1);
    }

    return 0;
}

/*for(i=0; i ++; i<10){
        printf("Hello %d times \n", i+1);
}*/          //========> Output Error
